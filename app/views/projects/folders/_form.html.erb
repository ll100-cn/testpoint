<%= f.input :name %>
<%= f.input :parent_id, collection: sort_folders(f.object.project.folders.ranked) if permit_attributes.include?(:parent_id) %>

<hr>

<%= f.actions_content do %>
  <%= ok_url_tag %>
  <%= f.submit class: "btn btn-primary" %>
  <%= cancel_link [f.object.project, ::TestCase, folder_id: f.object.id ], class: "btn btn-link" %>
<% end %>
